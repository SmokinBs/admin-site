{
    "compilerOptions": {
        "allowJs": true,
        //"checkJs": true,
        "alwaysStrict": true,
        "esModuleInterop": true,
        "forceConsistentCasingInFileNames": false,
        "isolatedModules": true,
        "jsx": "preserve",
        "lib": ["dom", "es2017"],
        "module": "esnext",
        "moduleResolution": "node",
        "noEmit": true,
        "noFallthroughCasesInSwitch": true,
        "noUnusedLocals": true,
        "noUnusedParameters": true,
        "resolveJsonModule": true,
        "skipLibCheck": true,
        "strict": true,
        "target": "esnext",
        "baseUrl": ".",
        "paths": {
            "*": ["src/*"]
        }
    },
    "exclude": ["node_modules"],
    "include": [
        "src/components/checkAuthStatus.js",
        "src/components/checkAuthStatus.jsx",
        "src/Layouts/Header.jsx",
        "src/pages/index.jsx",
        "next-env.d.ts",
        "**/*.ts",
        "**/*.ts",
        "src/pages/dashboard.jsx",
        "src/pages/people.jsx",
        "src/Examples/people.jsx"
    ]
}
